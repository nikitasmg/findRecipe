  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "serve": "pm2 start app.config.json",
    "test": "jest",
    "test:ci": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "eject": "react-scripts eject",
    "lint-staged": "lint-staged",
    "lint:es": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:es:fix": "npm run lint:es -- --fix",
    "pre-commit": "npm run lint-staged && npm run test",
    "pre-push": "npm run build",
    "changelog": "./scripts/changelog-update.sh",
    "install:git-hooks": "husky install && ./scripts/husky-update.sh",
    "generate:api": "graphql-codegen init",
    "license-checker:prod": "rimraf license-checker.json && license-checker --production --out license-checker.json --exclude 'MIT, MIT OR X11, BSD, ISC, Apache-2.0, CC0-1.0'",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0",
    "yarn": "!use npm!"
  }
